<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="captureVM" type="com.painrate.modules.capture.data.viewmodel.CaptureVM"/>
    </data>

    <LinearLayout
        android:id="@+id/linearColumnlogobluetext"
        style="@style/groupStylewhite_A700"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="start"
        android:orientation="vertical">

        <androidx.appcompat.widget.Toolbar
            android:id="@+id/toolbarToolbar"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_1pxh"
            android:gravity="center_horizontal"
            app:contentInsetStart="0dp">

            <LinearLayout
                android:id="@+id/linearTopBar"
                style="@style/groupStylewhite_1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/imageLogobluetext"
                    android:layout_width="@dimen/_134pxh"
                    android:layout_height="@dimen/_33pxh"
                    android:layout_marginStart="@dimen/_25pxh"
                    android:layout_marginTop="@dimen/_24pxv"
                    android:layout_marginBottom="@dimen/_16pxv"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_logobluetext"
                    tools:ignore="ContentDescription"
                    tools:src="@drawable/img_logobluetext" />

                <ImageView
                    android:id="@+id/imageCut"
                    android:layout_width="@dimen/_40pxh"
                    android:layout_height="@dimen/_29pxh"
                    android:layout_marginStart="@dimen/_173pxh"
                    android:layout_marginTop="@dimen/_22pxv"
                    android:layout_marginEnd="@dimen/_38pxh"
                    android:layout_marginBottom="@dimen/_23pxv"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_cut"
                    tools:ignore="ContentDescription"
                    tools:src="@drawable/img_cut" />
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>

        <LinearLayout
            android:id="@+id/linearCapture"
            style="@style/groupStylewhite_A700"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_horizontal"
            android:orientation="vertical">

            <LinearLayout
                android:id="@+id/linearColumnarrowleft"
                style="@style/groupStyleblack_901"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <ImageView
                    android:id="@+id/imageArrowleft"
                    android:layout_width="@dimen/_14pxh"
                    android:layout_height="@dimen/_14pxh"
                    android:layout_gravity="start"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_marginTop="@dimen/_32pxv"
                    android:layout_marginEnd="@dimen/_16pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_arrowleft_white_a700"
                    tools:ignore="ContentDescription"
                    tools:src="@drawable/img_arrowleft_white_a700" />

                <FrameLayout
                    android:id="@+id/frameStack1000f22043785"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_311pxv"
                    android:layout_marginTop="@dimen/_121pxv"
                    android:gravity="start">

                    <ImageView
                        android:id="@+id/myImageView"
                        imageRes="@{@drawable/img_1000f22043785}"
                        android:layout_width="@dimen/_411pxh"
                        android:layout_height="@dimen/_310pxh"
                        android:layout_gravity="center"
                        android:layout_marginStart="@dimen/_1pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_marginEnd="@dimen/_2pxh"
                        android:scaleType="fitXY"
                        tools:ignore="ContentDescription"
                        tools:src="@drawable/img_1000f22043785" />

                    <LinearLayout
                        android:id="@+id/linearColumnvectorone"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="top|start"
                        android:layout_marginTop="@dimen/_95pxv"
                        android:layout_marginBottom="@dimen/_95pxv"
                        android:orientation="vertical">

                        <View
                            android:id="@+id/lineVectorOne"
                            android:layout_width="@dimen/_413pxh"
                            android:layout_height="@dimen/_1pxv"
                            android:layout_gravity="center_horizontal"
                            android:layout_marginStart="@dimen/_1pxh"
                            android:background="@color/black_902" />

                        <View
                            android:id="@+id/lineVectorFour"
                            android:layout_width="@dimen/_414pxh"
                            android:layout_height="@dimen/_1pxv"
                            android:layout_marginTop="@dimen/_94pxv"
                            android:background="@color/gray_903" />
                    </LinearLayout>

                    <View
                        android:id="@+id/lineVectorThree"
                        android:layout_width="@dimen/_1pxh"
                        android:layout_height="@dimen/_311pxv"
                        android:layout_gravity="start"
                        android:layout_marginStart="@dimen/_161pxh"
                        android:layout_marginEnd="@dimen/_161pxh"
                        android:background="@color/gray_904" />

                    <View
                        android:id="@+id/lineVectorTwo"
                        android:layout_width="@dimen/_1pxh"
                        android:layout_height="@dimen/_311pxv"
                        android:layout_gravity="end"
                        android:layout_marginStart="@dimen/_156pxh"
                        android:layout_marginEnd="@dimen/_156pxh"
                        android:background="@color/black_903" />
                </FrameLayout>

                <LinearLayout
                    android:id="@+id/linearRowvideo"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_marginTop="@dimen/_15pxv"
                    android:layout_marginEnd="@dimen/_16pxh"
                    android:gravity="center_vertical"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/txtVideo"
                        style="@style/txtRobotoromanbold13"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_weight="0.22"
                        android:ellipsize="end"
                        android:singleLine="true"
                        android:text="@{captureVM.captureModel.txtVideo}"
                        tools:text="@string/lbl_video" />

                    <LinearLayout
                        android:id="@+id/linearRowphoto"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_53pxh"
                        android:layout_weight="0.78"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/txtPhoto"
                            style="@style/txtRobotoromanbold13_1"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_2pxv"
                            android:layout_weight="0.51"
                            android:ellipsize="end"
                            android:singleLine="true"
                            android:text="@{captureVM.captureModel.txtPhoto}"
                            tools:text="@string/lbl_photo" />

                        <TextView
                            android:id="@+id/txtVoice"
                            style="@style/txtRobotoromanbold13"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_52pxh"
                            android:layout_marginBottom="@dimen/_2pxv"
                            android:layout_weight="0.49"
                            android:ellipsize="end"
                            android:singleLine="true"
                            android:text="@{captureVM.captureModel.txtVoice}"
                            tools:text="@string/lbl_voice" />
                    </LinearLayout>
                </LinearLayout>

                <View
                    android:id="@+id/viewButton"
                    android:layout_width="@dimen/_60pxh"
                    android:layout_height="@dimen/_60pxh"
                    android:layout_gravity="center_horizontal"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_marginTop="@dimen/_30pxv"
                    android:layout_marginEnd="@dimen/_16pxh"
                    android:layout_marginBottom="@dimen/_20pxv"
                    android:background="@drawable/rectangle_bg_white_a700_radius_30" />
            </LinearLayout>

            <TextView
                android:id="@+id/textView"
                android:layout_width="match_parent"
                android:layout_height="42dp"
                android:gravity="center"
                android:text="PAIN RATE: "
                android:textColor="#000000"
                android:textSize="30sp"
                android:visibility="invisible" />

        </LinearLayout>

        <FrameLayout
            android:id="@+id/frameBottombar"
            style="@style/groupStylewhite_2"
            android:layout_width="fill_parent"
            android:layout_height="62dp"
            android:layout_gravity="end"
            android:layout_marginTop="@dimen/_40pxv"
            android:foregroundGravity="bottom">


            <LinearLayout
                android:id="@+id/linearRowgrid"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_marginBottom="@dimen/_10pxv"
                android:gravity="bottom"
                android:orientation="horizontal">

                <LinearLayout
                    android:id="@+id/linearDashboard"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_weight="1"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/imageGrid"
                        android:layout_width="@dimen/_20pxh"
                        android:layout_height="@dimen/_21pxh"
                        android:layout_gravity="center"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_grid"
                        tools:ignore="ContentDescription"
                        tools:src="@drawable/img_grid" />

                    <TextView
                        android:id="@+id/txtDashboard"
                        style="@style/txtPoppinsmedium10"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_2pxh"
                        android:layout_marginTop="@dimen/_3pxv"
                        android:layout_marginEnd="@dimen/_2pxh"
                        android:ellipsize="end"
                        android:gravity="center_horizontal"
                        android:singleLine="true"
                        android:text="@{captureVM.captureModel.txtDashboard}"
                        tools:text="@string/lbl_dashboard" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/linearCapture1"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_weight="1"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/imageClock"
                        android:layout_width="@dimen/_23pxh"
                        android:layout_height="@dimen/_21pxh"
                        android:layout_gravity="center"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_clock_light_blue_600"
                        tools:ignore="ContentDescription"
                        tools:src="@drawable/img_clock_light_blue_600" />

                    <TextView
                        android:id="@+id/txtCapture"
                        style="@style/txtPoppinsmedium10_1"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_2pxh"
                        android:layout_marginTop="@dimen/_4pxv"
                        android:layout_marginEnd="@dimen/_2pxh"
                        android:ellipsize="end"
                        android:gravity="center_horizontal"
                        android:singleLine="true"
                        android:text="@{captureVM.captureModel.txtCapture}"
                        tools:text="@string/lbl_capture" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/linearPatients"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_4pxv"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_weight="1"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/imageUser"
                        android:layout_width="@dimen/_27pxh"
                        android:layout_height="@dimen/_14pxh"
                        android:layout_gravity="center"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_user"
                        tools:ignore="ContentDescription"
                        tools:src="@drawable/img_user" />

                    <TextView
                        android:id="@+id/txtPatients"
                        style="@style/txtPoppinsmedium10_2"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_2pxh"
                        android:layout_marginTop="@dimen/_6pxv"
                        android:layout_marginEnd="@dimen/_2pxh"
                        android:ellipsize="end"
                        android:gravity="center_horizontal"
                        android:singleLine="true"
                        android:text="@{captureVM.captureModel.txtPatients}"
                        tools:text="@string/lbl_patients" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/linearNotification"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_weight="1"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/imageNotification"
                        android:layout_width="@dimen/_18pxh"
                        android:layout_height="@dimen/_23pxh"
                        android:layout_gravity="center"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_notification"
                        tools:ignore="ContentDescription"
                        tools:src="@drawable/img_notification" />

                    <TextView
                        android:id="@+id/txtNotificationOne"
                        style="@style/txtPoppinsmedium10"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_2pxh"
                        android:layout_marginTop="@dimen/_2pxv"
                        android:layout_marginEnd="@dimen/_2pxh"
                        android:ellipsize="end"
                        android:gravity="center_horizontal"
                        android:singleLine="true"
                        android:text="@{captureVM.captureModel.txtNotificationOne}"
                        tools:text="@string/lbl_notification" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/linearMenu"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_4pxv"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_weight="1"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/imageMenu"
                        android:layout_width="@dimen/_20pxh"
                        android:layout_height="@dimen/_14pxh"
                        android:layout_gravity="center"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_menu"
                        tools:ignore="ContentDescription"
                        tools:src="@drawable/img_menu" />

                    <TextView
                        android:id="@+id/txtMenuOne"
                        style="@style/txtPoppinsmedium10"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_2pxh"
                        android:layout_marginTop="@dimen/_7pxv"
                        android:layout_marginEnd="@dimen/_2pxh"
                        android:ellipsize="end"
                        android:gravity="center_horizontal"
                        android:singleLine="true"
                        android:text="@{captureVM.captureModel.txtMenuOne}"
                        tools:text="@string/lbl_menu" />
                </LinearLayout>
            </LinearLayout>
        </FrameLayout>

    </LinearLayout>
</layout>
